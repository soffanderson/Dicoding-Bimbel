{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": "airbnb-base",
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "implicit-arrow-linebreak": "off",
    "function-paren-newline": "off",
    "consistent-return": "off",
    "comma-dangle": "off",
    "linebreak-style": "off"
  }
}
